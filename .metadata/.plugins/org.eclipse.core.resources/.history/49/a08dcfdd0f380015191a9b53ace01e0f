package myPackage;

public class test {

	class Tree {
		public int x;
		public Tree l;
		public Tree r;
	}

	private int count(Tree t)
	{
		if(t== null) return 0;
		int l =1;
		l+=count(t.l);
		l+=count(t.r);
		return l;
	}

	private boolean inorder(Tree t, int A, int B)
	{
		if(t!= null && t.x=>A && t.x<=B) 
		{
			inorder(t.l);
			
		}
		else return false;
		
	}

